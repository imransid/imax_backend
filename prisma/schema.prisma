model User {
  id           Int      @id @default(autoincrement())
  fullName     String
  email        String   @unique
  mobileNumber String   @unique
  birthday     String
  gender       String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  password     String
}
